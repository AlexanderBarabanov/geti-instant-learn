{
    "name": "@geti/prompt",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "v24.2.0",
        "npm": "11.3.0"
    },
    "scripts": {
        "build:api:download": "curl -o ./src/api/openapi-spec.json http://localhost:9100/api/openapi.json && prettier --write ./src/api/openapi-spec.json",
        "build:api:types": "npx openapi-typescript ./src/api/openapi-spec.json -o ./src/api/openapi-spec.d.ts --root-types && prettier --write ./src/api/openapi-spec.d.ts",
        "build:api": "npm run build:api:download && npm run build:api:types",
        "build": "rsbuild build",
        "cyclic-deps-check": "npx madge --circular ./**/*.ts*",
        "dev": "concurrently -n Server,UI -c blue,yellow \"npm run server\" \"npm start\"",
        "start": "rsbuild dev --open",
        "server": "uv --directory ../backend/app run main.py",
        "format": "prettier --write .",
        "format:check": "prettier --check --cache \"./**/*.{js,ts,jsx,tsx,css,scss,sass}\"",
        "lint": "eslint ./src ./tests --max-warnings 0",
        "lint:fix": "eslint --fix ./src ./tests",
        "preview": "rsbuild preview",
        "clone-geti-ui-packages": "npx tiged --force --mode=git https://github.com/open-edge-platform/geti.git/web_ui/packages/config#e84dbb794fc7ce836ee4b58674baec8e5db8c5fd packages/config && npx tiged --force --mode=git https://github.com/open-edge-platform/geti.git/web_ui/packages/ui#e84dbb794fc7ce836ee4b58674baec8e5db8c5fd packages/ui",
        "preinstall": "npm run clone-geti-ui-packages",
        "type-check": "concurrently -n UI,Tests -c green,cyan \"npm run type-check:ui\" \"npm run type-check:tests\"",
        "type-check:ui": "tsc --noEmit -p ./tsconfig.json",
        "type-check:tests": "tsc --noEmit -p ./tests/tsconfig.json",
        "test:unit": "vitest",
        "test:unit:ui": "vitest --ui --watch=true",
        "test:unit:coverage": "vitest --coverage",
        "test:unit:watch": "vitest --watch=true",
        "test:component": "npx playwright test --config=playwright.config.ts"
    },
    "dependencies": {
        "@geti/ui": "*",
        "@tanstack/react-query": "^5.87.0",
        "@use-gesture/react": "^10.3.1",
        "lodash-es": "^4.17.21",
        "openapi-fetch": "^0.14.0",
        "openapi-react-query": "^0.5.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-router": "^6.30.1",
        "react-router-dom": "^6.30.1",
        "static-path": "^0.0.4",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.8",
        "@eslint/js": "^9.30.0",
        "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
        "@msw/playwright": "^0.4.2",
        "@mswjs/source": "^0.5.0",
        "@playwright/test": "^1.55.0",
        "@rsbuild/core": "^1.4.13",
        "@rsbuild/plugin-babel": "^1.0.6",
        "@rsbuild/plugin-react": "^1.3.4",
        "@rsbuild/plugin-sass": "^1.3.5",
        "@rsbuild/plugin-svgr": "^1.2.2",
        "@tanstack/eslint-plugin-query": "^5.78.0",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "^24.3.0",
        "@types/react": "^19.1.9",
        "@types/react-dom": "^19.1.7",
        "@typescript-eslint/eslint-plugin": "^8.38.0",
        "@vitejs/plugin-react": "^5.0.2",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "babel-plugin-react-compiler": "^19.1.0-rc.3",
        "concurrently": "^9.2.1",
        "eslint": "^9.30.0",
        "eslint-import-resolver-typescript": "^4.3.4",
        "eslint-plugin-headers": "^1.3.3",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-playwright": "^2.2.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^6.0.0-rc.2",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^11.12.0",
        "jsdom": "^26.1.0",
        "msw": "^2.11.3",
        "node-fetch": "^3.3.2",
        "openapi-msw": "^2.0.0",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.35.1",
        "vite-plugin-svgr": "^4.5.0",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    },
    "workspaces": [
        "packages/*"
    ]
}
